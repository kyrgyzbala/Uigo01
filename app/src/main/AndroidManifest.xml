<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="kg.kyrgyzcoder.kassa01">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="kg.kyrgyzcoder.kassa01.util.UigoApp"
        android:allowBackup="true"
        android:icon="@mipmap/new_icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/new_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        tools:targetApi="n">
        <activity android:name="kg.kyrgyzcoder.kassa01.data.network.sell.ReceiptActivity" />
        <activity android:name="kg.kyrgyzcoder.kassa01.ui.items.ItemEditActivity" />
        <activity android:name="kg.kyrgyzcoder.kassa01.ui.more.AddNewEmplActivity" />
        <activity android:name="kg.kyrgyzcoder.kassa01.ui.login.CashierLoginActivity" />
        <activity android:name="kg.kyrgyzcoder.kassa01.ui.items.util.ScanItemActivity" />
        <activity android:name="kg.kyrgyzcoder.kassa01.ui.items.ItemsActivity" />
        <activity android:name="kg.kyrgyzcoder.kassa01.ui.login.RegisterActivity" />
        <activity android:name="kg.kyrgyzcoder.kassa01.ui.login.LoginActivity" />
        <activity android:name="kg.kyrgyzcoder.kassa01.ui.orders.detail.OrderDetailActivity" />
        <activity android:name="kg.kyrgyzcoder.kassa01.ui.main.MainActivity" />
        <activity android:name="kg.kyrgyzcoder.kassa01.ui.splash.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>